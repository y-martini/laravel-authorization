{
    "name": "yuriy-martini/laravel-authorization",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10"
    },
    "require-dev": {
        "orchestra/testbench": "^8",
        "phpstan/phpstan": "^1",
        "squizlabs/php_codesniffer": "^3"
    },
    "autoload": {
        "psr-4": {
            "YuriyMartini\\Laravel\\Authorization\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "YuriyMartini\\Laravel\\Authorization\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "YuriyMartini\\Laravel\\Authorization\\ServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
